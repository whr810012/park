<view class="rf-product-detail"><text data-event-opts="{{[['tap',[['navBack',['$event']]]]]}}" class="back-btn iconfont iconzuo" bindtap="__e"></text><block wx:if="{{product.stallId}}"><view class="detail"><view class="carousel"><swiper indicator-dots="{{true}}" circular="true" duration="400" controls="{{true}}" touchable="{{true}}"><block wx:for="{{product.url}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="swiper-item"><view class="image-wrapper"><image class="loaded" src="{{item}}" mode="aspectFill"></image></view><uni-tag class="content" vue-id="{{'3a07b446-1-'+index}}" circle="{{true}}" size="small" text="{{index+1+'  / '+product.url.length}}" bind:__l="__l"></uni-tag></swiper-item></block></swiper></view><view class="introduce-section"><view class="introduce-first-line"><view class="price-box"><view class="price-first-line"><text class="{{['price','text-'+themeColor.name]}}">{{moneySymbol+product.price}}</text></view><block wx:if="{{product.market_price>product.price}}"><view class="m-price-wrapper">价格<text class="m-price">{{moneySymbol+product.price}}</text></view></block></view></view><view class="introduce-second-line"><view class="title"><text>{{product.areaName}}</text><text class="sketch">{{product.location}}</text></view><view class="share"><rf-tag vue-id="3a07b446-2" type="gray" size="small" tui-tag-class="tui-tag-share tui-size" shape="circleLeft" bind:__l="__l" vue-slots="{{['default']}}"><button class="{{['share-btn','text-'+themeColor.name]}}" open-type="share" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" catchtap="__e"><text class="iconfont iconfenxiang"></text><text class="tui-share-text tui-gray">分享</text></button></rf-tag></view></view><view class="data"><text class="item">{{"状态："+(product.status==0?'空闲':product.status==1?'预定中':'已占用')}}</text></view><view style="display:flex;justify-content:space-between;align-items:center;"><view style="width:200rpx;text-align:right;">开始停车时间：</view><uni-datetime-picker bind:input="__e" vue-id="3a07b446-3" returnType="timestamp" value="{{startDate}}" data-event-opts="{{[['^input',[['__set_model',['','startDate','$event',[]]]]]]}}" bind:__l="__l"></uni-datetime-picker></view><view style="display:flex;justify-content:space-between;align-items:center;padding-top:20px;"><view style="width:200rpx;text-align:right;">离开时间：</view><uni-datetime-picker bind:input="__e" vue-id="3a07b446-4" returnType="timestamp" value="{{endDate}}" data-event-opts="{{[['^input',[['__set_model',['','endDate','$event',[]]]]]]}}" bind:__l="__l"></uni-datetime-picker></view><view style="display:flex;align-items:center;padding-bottom:20px;padding-top:20px;"><view style="width:200rpx;text-align:right;">是否预定：</view><switch checked="{{status}}" data-event-opts="{{[['change',[['changeSwitch',['$event']]]]]}}" bindchange="__e"></switch></view></view><view class="c-list"></view><view data-event-opts="{{[['tap',[['toEvaluateList',['$event']]]]]}}" class="eva-section" bindtap="__e"><view class="e-header"><text class="tit">{{"评价("+(evaluateList.length||0)+")"}}</text><block wx:if="{{!evaluateList.length}}"><text class="tip">暂无评价信息</text></block><view class="iconfont iconyou _i"></view></view><block wx:if="{{evaluateList.length>0}}"><view class="eva-box"><view class="right"><view class="name"><text>{{''+(evaluateList[0].userName||'匿名用户')+''}}</text><block wx:if="{{evaluateList.length>0}}"><rf-rate vue-id="3a07b446-5" size="16" disabled="true" value="{{evaluateList[0].score}}" active-color="{{themeColor.color}}" bind:__l="__l"></rf-rate></block></view><text class="con in2line">{{evaluateList[0].body||'这个人很懒，什么都没留下~'}}</text><view class="bot"><text class="time">{{evaluateList[0].createTime}}</text></view></view></view></block></view><view class="page-bottom"><view class="page-bottom-bth-wrapper"><navigator class="p-b-btn" url="/pages/index/index" open-type="switchTab"><view class="iconfont iconzhuyedefuben _i"></view><text>首页</text></navigator></view><view class="action-btn-group"><button class="{{['action-btn','bg-'+themeColor.name]}}" disabled="{{buyBtnDisabled}}" data-event-opts="{{[['tap',[['buy']]]]}}" bindtap="__e">立即购买</button></view></view></view></block></view>