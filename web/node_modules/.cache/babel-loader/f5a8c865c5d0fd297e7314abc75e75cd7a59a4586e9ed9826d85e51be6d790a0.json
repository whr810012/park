   console.log(this.$route.query.goodsId);\n    this.init();\n  }\n};","map":{"version":3,"names":["getGoods","name","data","statusList","value","label","goodsDetail","options","options2","imageshow","dialogImageUrl","allGoodsList","loading","methods","handlePictureCardPreview","file","url","init","then","res","console","log","forEach","item","image","JSON","parse","filesJson","img","mimeType","contentType","base64","find","goodsId","$route","query","created","$store","commit","title"],"sources":["C:\\Users\\Administrator\\Desktop\\ershou\\src\\pages\\goodsDetail.vue"],"sourcesContent":["<template>\r\n    <div class=\"goodsDetail\" v-if=\"goodsDetail\" v-loading=\"!goodsDetail\">\r\n        <el-form disabled :model=\"goodsDetail\" style=\"padding-left: 20px;padding-top: 10px;\" label-width=\"100px\">\r\n            <el-form-item label=\"名称：\">\r\n                <el-input style=\"width: 400px;\" placeholder=\"请填写商品名称\" v-model=\"goodsDetail.name\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"状态：\">\r\n                <span style=\"font-size: 16px;font-weight: 600;color:red\">\r\n                {{ this.statusList.find(status => status.value === goodsDetail.status).label }}\r\n                </span>\r\n            </el-form-item>\r\n            <el-form-item label=\"原价：\">\r\n                <el-input-number v-model=\"goodsDetail.price\" style=\"width: 400px;\" min=\"0\" :precision=\"2\"\r\n                    :step=\"0.1\"></el-input-number>\r\n            </el-form-item>\r\n            <el-form-item label=\"折扣：\">\r\n                <el-input-number v-model=\"goodsDetail.discount\" style=\"width: 400px;\" :precision=\"2\" :step=\"0.01\"\r\n                    max=\"1\" min=\"0\"></el-input-number>\r\n                <span v-if=\"goodsDetail.discount !== 1\" style=\"margin-left: 10px;\">\r\n                    {{ goodsDetail.discount * 100 / 10 }}折\r\n                </span>\r\n            </el-form-item>\r\n            <el-form-item label=\"售价：\">\r\n                <el-input disabled style=\"width: 400px;\"\r\n                    :value=\"(goodsDetail.price * (goodsDetail.discount * 100)) / 100\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"分类：\">\r\n                <el-select v-model=\"goodsDetail.category\" placeholder=\"请选择商品分类\" style=\"width: 240px\">\r\n                    <el-option v-for=\"item in options\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" />\r\n                </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"成色\">\r\n                <el-select v-model=\"goodsDetail.condition\" placeholder=\"请选择商品成色\" style=\"width: 240px\">\r\n                    <el-option v-for=\"item in options2\" :key=\"item\" :label=\"item\" :value=\"item\" />\r\n                </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"交易详细地址:\">\r\n                    <el-input v-model=\"goodsDetail.address\" style=\"width: 400px\" :disabled=\"goodsDetail.address === ''\"></el-input>\r\n                    {{ goodsDetail.address }}\r\n                </el-form-item>\r\n            <el-form-item label=\"商品描述：\">\r\n                <div style=\"display: flex;\">\r\n                    <el-input type=\"textarea\" v-loading=\"loading\" v-model=\"goodsDetail.description\"\r\n                        style=\"width: 400px;\" :rows=\"6\" placeholder=\"请填写商品描述\">\r\n                    </el-input>\r\n                </div>\r\n            </el-form-item>\r\n            \r\n            <el-form-item label=\"图片:\">\r\n                <el-upload action=\"#\" v-model:file-list=\"goodsDetail.image\" :auto-upload=\"false\" limit=\"4\"\r\n                    list-type=\"picture-card\" :on-preview=\"handlePictureCardPreview\" :on-exceed=\"handleExceed\"\r\n                    accept=\"image/jpg,image/jpeg,image/png\">\r\n                </el-upload>\r\n                <el-dialog v-model=\"imageshow\">\r\n                    <img style=\"width: 100%;\" w-full :src=\"dialogImageUrl\" alt=\"Preview Image\" />\r\n                </el-dialog>\r\n            </el-form-item>\r\n        </el-form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { getGoods } from '../api/request'\r\nexport default {\r\n    name: 'goodsDetail',\r\n    data() {\r\n        return {\r\n            statusList: [\r\n                { value: 0, label: '待审核' },\r\n                { value: 1, label: '上架中' },\r\n                { value: 2, label: '已下架' },\r\n                { value: 3, label: '订单交易中' },\r\n                { value: 4, label: '已售出' }\r\n            ],\r\n            goodsDetail: null,\r\n            options: [\r\n                { label: '书籍', value: 'book' },\r\n                { label: '电子产品', value: 'electronics' },\r\n                { label: '食品', value: 'food' },\r\n                { label: '衣物', value: 'clothes' },\r\n                { label: '家具', value: 'furniture' },\r\n                { label: '化妆品', value: 'cosmetics' },\r\n                { label: '运动器材', value: 'sports' },\r\n                { label: '乐器', value: 'instrument' },\r\n                { label: '其他', value: 'other' }\r\n            ],\r\n            options2: ['全新', '九成新', '八成新', '七成新', '六成新', '五成新', '四成新', '三成新', '二成新', '一成新'],\r\n            imageshow: false,\r\n            dialogImageUrl: '',\r\n            allGoodsList: [],\r\n            loading: false\r\n        }\r\n    },\r\n    methods: {\r\n        handlePictureCardPreview(file) {\r\n            this.imageshow = true\r\n            this.dialogImageUrl = file.url\r\n        },\r\n        init() {\r\n            getGoods().then(res => {\r\n                console.log(res.data.data)\r\n                this.allGoodsList = res.data.data\r\n                this.allGoodsList.forEach(item => {\r\n                    item.image = JSON.parse(item.filesJson)\r\n                    item.image.forEach(img => {\r\n                        const mimeType = img.contentType\r\n                        img.url = `data:${mimeType};base64,${img.base64}`;\r\n                    })\r\n                })\r\n                this.goodsDetail = this.allGoodsList.find(item => item.goodsId == this.$route.query.goodsId)\r\n                console.log(this.goodsDetail);\r\n            })\r\n        }\r\n    },\r\n    created() {\r\n        \r\n        this.$store.commit('setBreadList', [{ title: '商品详情' }])\r\n        console.log(this.$route.query.goodsId);\r\n        this.init()\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.goodsDetail {\r\n    margin: 20px;\r\n}\r\n</style>"],"mappings":"AA8DA,SAASA,QAAO,QAAS,gBAAe;AACxC,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,UAAU,EAAE,CACR;QAAEC,KAAK,EAAE,CAAC;QAAEC,KAAK,EAAE;MAAM,CAAC,EAC1B;QAAED,KAAK,EAAE,CAAC;QAAEC,KAAK,EAAE;MAAM,CAAC,EAC1B;QAAED,KAAK,EAAE,CAAC;QAAEC,KAAK,EAAE;MAAM,CAAC,EAC1B;QAAED,KAAK,EAAE,CAAC;QAAEC,KAAK,EAAE;MAAQ,CAAC,EAC5B;QAAED,KAAK,EAAE,CAAC;QAAEC,KAAK,EAAE;MAAM,EAC5B;MACDC,WAAW,EAAE,IAAI;MACjBC,OAAO,EAAE,CACL;QAAEF,KAAK,EAAE,IAAI;QAAED,KAAK,EAAE;MAAO,CAAC,EAC9B;QAAEC,KAAK,EAAE,MAAM;QAAED,KAAK,EAAE;MAAc,CAAC,EACvC;QAAEC,KAAK,EAAE,IAAI;QAAED,KAAK,EAAE;MAAO,CAAC,EAC9B;QAAEC,KAAK,EAAE,IAAI;QAAED,KAAK,EAAE;MAAU,CAAC,EACjC;QAAEC,KAAK,EAAE,IAAI;QAAED,KAAK,EAAE;MAAY,CAAC,EACnC;QAAEC,KAAK,EAAE,KAAK;QAAED,KAAK,EAAE;MAAY,CAAC,EACpC;QAAEC,KAAK,EAAE,MAAM;QAAED,KAAK,EAAE;MAAS,CAAC,EAClC;QAAEC,KAAK,EAAE,IAAI;QAAED,KAAK,EAAE;MAAa,CAAC,EACpC;QAAEC,KAAK,EAAE,IAAI;QAAED,KAAK,EAAE;MAAQ,EACjC;MACDI,QAAQ,EAAE,CAAC,IAAI,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,CAAC;MAC/EC,SAAS,EAAE,KAAK;MAChBC,cAAc,EAAE,EAAE;MAClBC,YAAY,EAAE,EAAE;MAChBC,OAAO,EAAE;IACb;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,wBAAwBA,CAACC,IAAI,EAAE;MAC3B,IAAI,CAACN,SAAQ,GAAI,IAAG;MACpB,IAAI,CAACC,cAAa,GAAIK,IAAI,CAACC,GAAE;IACjC,CAAC;IACDC,IAAIA,CAAA,EAAG;MACHjB,QAAQ,CAAC,CAAC,CAACkB,IAAI,CAACC,GAAE,IAAK;QACnBC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACjB,IAAI,CAACA,IAAI;QACzB,I