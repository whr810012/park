{"ast":null,"code":"import { getAllOrder } from '../../api/request';\nimport * as echarts from 'echarts';\nexport default {\n  data() {\n    return {\n      orderList: [],\n      notOrderList: [],\n      awaitOrderList: [],\n      okOrderList: []\n    };\n  },\n  methods: {\n    init() {\n      getAllOrder().then(res => {\n        this.orderList = res.data.data;\n        console.log(this.orderList);\n        this.okOrderList = this.orderList.filter(item => item.status === '1' || item.status === '3');\n        this.notOrderList = this.orderList.filter(item => item.status === '2');\n        this.awaitOrderList = this.orderList.filter(item => item.status === '0');\n      });\n    }\n  },\n  computed: {\n    proportionOption() {\n      return {\n        title: {\n          text: 'Referer of a Website',\n          subtext: 'Fake Data',\n          left: 'center'\n        },\n        tooltip: {\n          trigger: 'item'\n        },\n        legend: {\n          orient: 'vertical',\n          left: 'left'\n        },\n        series: [{\n          name: '数量',\n          type: 'pie',\n          data: [{\n            value: this.okOrderList.length,\n            name: '已确认、已完成订单'\n          }, {\n            value: this.notOrderList.length,\n            name: '已拒绝订单'\n          }, {\n            value: this.awaitOrderList.length,\n            name: '待确认订单'\n          }]\n        }]\n      };\n    }\n  },\n  created() {\n    this.init();\n  },\n  mounted() {\n    const myChart = echarts.init(document.getElementById('proportionOption'));\n    myChart.setOption(this.proportionOption);\n  }\n};","map":{"version":3,"names":["getAllOrder","echarts","data","orderList","notOrderList","awaitOrderList","okOrderList","methods","init","then","res","console","log","filter","item","status","computed","proportionOption","title","text","subtext","left","tooltip","trigger","legend","orient","series","name","type","value","length","created","mounted","myChart","document","getElementById","setOption"],"sources":["C:\\Users\\Administrator\\Desktop\\ershou\\src\\pages\\home\\statisticsManage.vue"],"sourcesContent":["<template>\r\n  <div style=\"padding: 20px\">\r\n    <div class=\"title\">\r\n      各类型订单比例示意图\r\n    </div>\r\n    <div class=\"proportionOption\" id=\"proportionOption\" style=\"width: 600px;height:400px\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getAllOrder } from '../../api/request'\r\nimport * as echarts from 'echarts';\r\nexport default {\r\n  data() {\r\n    return {\r\n      orderList: [],\r\n      notOrderList: [],\r\n      awaitOrderList: [],\r\n      okOrderList: []\r\n    }\r\n  },\r\n  methods: {\r\n    init() {\r\n      getAllOrder().then(res => {\r\n        this.orderList = res.data.data\r\n        console.log(this.orderList)\r\n        this.okOrderList = this.orderList.filter(item => item.status === '1' || item.status === '3')\r\n        this.notOrderList = this.orderList.filter(item => item.status === '2')\r\n        this.awaitOrderList = this.orderList.filter(item => item.status === '0')\r\n      })\r\n    }\r\n  },\r\n  computed: {\r\n    proportionOption() {\r\n      return {\r\n        title: {\r\n          text: 'Referer of a Website',\r\n          subtext: 'Fake Data',\r\n          left: 'center'\r\n        },\r\n        tooltip: {\r\n          trigger: 'item'\r\n        },\r\n        legend: {\r\n          orient: 'vertical',\r\n          left: 'left'\r\n        },\r\n        series: [\r\n          {\r\n            name: '数量',\r\n            type: 'pie',\r\n            data: [\r\n              {\r\n                value: this.okOrderList.length,\r\n                name: '已确认、已完成订单'\r\n              },\r\n              {\r\n                value: this.notOrderList.length,\r\n                name: '已拒绝订单'\r\n              },\r\n              {\r\n                value: this.awaitOrderList.length,\r\n                name: '待确认订单'\r\n              }\r\n            ]\r\n          }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.init()\r\n  },\r\n  mounted() {\r\n    const myChart = echarts.init(document.getElementById('proportionOption'))\r\n    myChart.setOption(this.proportionOption)\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.title {\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n  margin-bottom: 20px;\r\n}\r\n</style>"],"mappings":"AAUA,SAASA,WAAU,QAAS,mBAAkB;AAC9C,OAAO,KAAKC,OAAM,MAAO,SAAS;AAClC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE,EAAE;MAChBC,cAAc,EAAE,EAAE;MAClBC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,IAAIA,CAAA,EAAG;MACLR,WAAW,CAAC,CAAC,CAACS,IAAI,CAACC,GAAE,IAAK;QACxB,IAAI,CAACP,SAAQ,GAAIO,GAAG,CAACR,IAAI,CAACA,IAAG;QAC7BS,OAAO,CAACC,GAAG,CAAC,IAAI,CAACT,SAAS;QAC1B,IAAI,CAACG,WAAU,GAAI,IAAI,CAACH,SAAS,CAACU,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,MAAK,KAAM,GAAE,IAAKD,IAAI,CAACC,MAAK,KAAM,GAAG;QAC3F,IAAI,CAACX,YAAW,GAAI,IAAI,CAACD,SAAS,CAACU,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,MAAK,KAAM,GAAG;QACrE,IAAI,CAACV,cAAa,GAAI,IAAI,CAACF,SAAS,CAACU,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,MAAK,KAAM,GAAG;MACzE,CAAC;IACH;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,OAAO;QACLC,KAAK,EAAE;UACLC,IAAI,EAAE,sBAAsB;UAC5BC,OAAO,EAAE,WAAW;UACpBC,IAAI,EAAE;QACR,CAAC;QACDC,OAAO,EAAE;UACPC,OAAO,EAAE;QACX,CAAC;QACDC,MAAM,EAAE;UACNC,MAAM,EAAE,UAAU;UAClBJ,IAAI,EAAE;QACR,CAAC;QACDK,MAAM,EAAE,CACN;UACEC,IAAI,EAAE,IAAI;UACVC,IAAI,EAAE,KAAK;UACX1B,IAAI,EAAE,CACJ;YACE2B,KAAK,EAAE,IAAI,CAACvB,WAAW,CAACwB,MAAM;YAC9BH,IAAI,EAAE;UACR,CAAC,EACD;YACEE,KAAK,EAAE,IAAI,CAACzB,YAAY,CAAC0B,MAAM;YAC/BH,IAAI,EAAE;UACR,CAAC,EACD;YACEE,KAAK,EAAE,IAAI,CAACxB,cAAc,CAACyB,MAAM;YACjCH,IAAI,EAAE;UACR;QAEJ;MAEJ;IACF;EACF,CAAC;EACDI,OAAOA,CAAA,EAAG;IACR,IAAI,CAACvB,IAAI,CAAC;EACZ,CAAC;EACDwB,OAAOA,CAAA,EAAG;IACR,MAAMC,OAAM,GAAIhC,OAAO,CAACO,IAAI,CAAC0B,QAAQ,CAACC,cAAc,CAAC,kBAAkB,CAAC;IACxEF,OAAO,CAACG,SAAS,CAAC,IAAI,CAACnB,gBAAgB;EACzC;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}