{"ast":null,"code":"import { getAllOrder, getAllArea, getOrderDetail, updateOrder } from \"@/api/request\";\nimport dayjs from \"dayjs\";\nexport default {\n  data() {\n    return {\n      TableList: [],\n      currentPage: 1,\n      drawer: false,\n      orderInfo: {},\n      areaList: [],\n      stallList: [],\n      userList: []\n    };\n  },\n  computed: {\n    filterList() {\n      return this.TableList.filter((item, index) => {\n        return index < this.currentPage * 10 && index >= (this.currentPage - 1) * 10;\n      });\n    }\n  },\n  methods: {\n    giveDiscount(row) {\n      this.$prompt(\"请输入折扣(0.01~1之间，例如0.9代表九折)\", \"提示\", {}).then(({\n        value\n      }) => {\n        const data = {\n          orderid: row.orderid,\n          discount: value\n        };\n        updateOrder(data).then(() => {\n          this.init();\n          this.$message({\n            type: \"success\",\n            message: \"折扣为: \" + value\n          });\n        });\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"取消输入\"\n        });\n      });\n    },\n    handleSelectionChange(val) {\n      this.checkGoods = val;\n    },\n    lookDetail(row) {\n      getOrderDetail(row.orderid).then(res => {\n        console.log();\n        this.orderInfo = res.data.data;\n        this.orderInfo.createTime = dayjs(this.orderInfo.createTime).format(\"YYYY-MM-DD HH:mm:ss\");\n        this.orderInfo.startTime = dayjs(this.orderInfo.startTime).format(\"YYYY-MM-DD HH:mm:ss\");\n        this.orderInfo.outTime = dayjs(this.orderInfo.outTime).format(\"YYYY-MM-DD HH:mm:ss\");\n        this.orderInfo.endTime = dayjs(this.orderInfo.endTime).format(\"YYYY-MM-DD HH:mm:ss\");\n        this.areaList.map(area => {\n          if (this.orderInfo.areaid === area.areaid) {\n            this.orderInfo.areaName = area.areaName;\n          }\n        });\n        this.drawer = true;\n      });\n    },\n    init() {\n      getAllOrder().then(res => {\n        console.log(res.data.data);\n        this.TableList = res.data.data;\n        this.TableList.forEach(item => {\n          item.createTime = dayjs(item.createTime).format(\"YYYY-MM-DD HH:mm:ss\");\n          item.outTime = dayjs(item.outTime).format(\"YYYY-MM-DD HH:mm:ss\");\n          item.startTime = dayjs(item.startTime).format(\"YYYY-MM-DD HH:mm:ss\");\n          item.endTime = dayjs(item.endTime).format(\"YYYY-MM-DD HH:mm:ss\");\n          this.areaList.map(area => {\n            if (item.areaId === area.areaid) {\n              item.areaName = area.areaName;\n            }\n          });\n        });\n      });\n    }\n  },\n  created() {\n    getAllArea().then(res => {\n      console.log(res.data.data);\n      this.areaList = res.data.data;\n    });\n    this.init();\n  }\n};","map":{"version":3,"names":["getAllOrder","getAllArea","getOrderDetail","updateOrder","dayjs","data","TableList","currentPage","drawer","orderInfo","areaList","stallList","userList","computed","filterList","filter","item","index","methods","giveDiscount","row","$prompt","then","value","orderid","discount","init","$message","type","message","catch","handleSelectionChange","val","checkGoods","lookDetail","res","console","log","createTime","format","startTime","outTime","endTime","map","area","areaid","areaName","forEach","areaId","created"],"sources":["D:\\CODE\\park\\park-vue\\web\\src\\pages\\home\\ordersManage.vue"],"sourcesContent":["<template>\r\n  <div class=\"goodsManage\">\r\n    <el-table :data=\"filterList\" style=\"width: 100%\" border>\r\n      <el-table-column\r\n        label=\"序号\"\r\n        width=\"120\"\r\n        align=\"center\"\r\n        type=\"index\"\r\n      ></el-table-column>\r\n      <el-table-column property=\"orderid\" label=\"订单号\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column property=\"createTime\" label=\"创建时间\"></el-table-column>\r\n      <el-table-column prop=\"outTime\" label=\"离场时间\"></el-table-column>\r\n      <el-table-column prop=\"startTime\" label=\"开始时间\"></el-table-column>\r\n      <el-table-column prop=\"endTime\" label=\"结束时间\"></el-table-column>\r\n      <el-table-column prop=\"totalPrice\" label=\"总价格\"></el-table-column>\r\n      <el-table-column prop=\"discount\" label=\"折扣\">\r\n        <template #default=\"scope\">\r\n          <span v-if=\"scope.row.discount == 1\">无折扣</span>\r\n          <span v-else>{{ scope.row.discount * 10 }}折</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"status\" label=\"状态\">\r\n        <template #default=\"scope\">\r\n          <el-tag type=\"primary\" v-if=\"scope.row.status === 0\">预定中</el-tag>\r\n          <el-tag type=\"success\" v-if=\"scope.row.status === 1\">进行中</el-tag>\r\n          <el-tag type=\"danger\" v-if=\"scope.row.status === 2\">已超时</el-tag>\r\n          <el-tag type=\"info\" v-if=\"scope.row.status === 3\">已完成</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\r\n        <template #default=\"scope\">\r\n          <el-button type=\"text\" size=\"small\" @click=\"lookDetail(scope.row)\">查看详情</el-button>\r\n          <el-button type=\"text\" size=\"small\" @click=\"giveDiscount(scope.row)\">折扣设置</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"margin-top: 20px\">\r\n      <el-pagination\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :total=\"TableList.length\"\r\n        v-model:current-page=\"currentPage\"\r\n      ></el-pagination>\r\n    </div>\r\n    <el-drawer\r\n      v-model=\"drawer\"\r\n      title=\"I am the title\"\r\n      :with-header=\"false\"\r\n      width=\"300px\"\r\n    >\r\n      <h3>订单详情</h3>\r\n      <div style=\"margin-top: 10px\">订单号： {{ orderInfo.orderid }}</div>\r\n      <div style=\"margin-top: 10px\">买家： {{ orderInfo.userName }}</div>\r\n      <div style=\"margin-top: 10px\">车辆信息： {{ orderInfo.carInfo }}</div>\r\n      <div style=\"margin-top: 10px\">\r\n        停放位置： {{ orderInfo.areaName }} - {{ orderInfo.location }}\r\n      </div>\r\n      <div style=\"margin-top: 10px\">\r\n        订单创建时间： {{ orderInfo.createTime }}\r\n      </div>\r\n      <div style=\"margin-top: 10px\">\r\n        订单开始时间： {{ orderInfo.startTime }}\r\n      </div>\r\n      <div style=\"margin-top: 10px\">订单结束时间： {{ orderInfo.endTime }}</div>\r\n      <div style=\"margin-top: 10px\">车主离开时间： {{ orderInfo.outTime }}</div>\r\n      <div style=\"margin-top: 10px\">\r\n        订单状态：\r\n        <span>\r\n          <el-tag type=\"primary\" v-if=\"orderInfo.status === 0\">预定中</el-tag>\r\n          <el-tag type=\"success\" v-if=\"orderInfo.status === 1\">进行中</el-tag>\r\n          <el-tag type=\"danger\" v-if=\"orderInfo.status === 2\">已超时</el-tag>\r\n          <el-tag type=\"info\" v-if=\"orderInfo.status === 3\">已完成</el-tag>\r\n        </span>\r\n      </div>\r\n    </el-drawer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  getAllOrder,\r\n  getAllArea,\r\n  getOrderDetail,\r\n  updateOrder\r\n} from \"@/api/request\";\r\nimport dayjs from \"dayjs\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      TableList: [],\r\n      currentPage: 1,\r\n      drawer: false,\r\n      orderInfo: {},\r\n      areaList: [],\r\n      stallList: [],\r\n      userList: [],\r\n    };\r\n  },\r\n  computed: {\r\n    filterList() {\r\n      return this.TableList.filter((item, index) => {\r\n        return (\r\n          index < this.currentPage * 10 && index >= (this.currentPage - 1) * 10\r\n        );\r\n      });\r\n    },\r\n  },\r\n  methods: {\r\n    giveDiscount(row) {\r\n      this.$prompt(\"请输入折扣(0.01~1之间，例如0.9代表九折)\", \"提示\", {\r\n      }).then(({ value }) => {\r\n        const data = {\r\n        orderid: row.orderid,\r\n        discount: value\r\n      }\r\n        updateOrder(data).then(() => {\r\n          this.init()\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"折扣为: \" + value,\r\n          });\r\n        })\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: \"info\",\r\n          message: \"取消输入\",\r\n        });\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      this.checkGoods = val;\r\n    },\r\n    lookDetail(row) {\r\n      getOrderDetail(row.orderid).then((res) => {\r\n        console.log();\r\n        this.orderInfo = res.data.data;\r\n        this.orderInfo.createTime = dayjs(this.orderInfo.createTime).format(\"YYYY-MM-DD HH:mm:ss\")\r\n        this.orderInfo.startTime = dayjs(this.orderInfo.startTime).format(\"YYYY-MM-DD HH:mm:ss\")\r\n        this.orderInfo.outTime = dayjs(this.orderInfo.outTime).format(\"YYYY-MM-DD HH:mm:ss\")\r\n        this.orderInfo.endTime = dayjs(this.orderInfo.endTime).format(\"YYYY-MM-DD HH:mm:ss\")\r\n        this.areaList.map((area) => {\r\n          if (this.orderInfo.areaid === area.areaid) {\r\n            this.orderInfo.areaName = area.areaName;\r\n          }\r\n        });\r\n        this.drawer = true;\r\n      });\r\n    },\r\n    init() {\r\n      getAllOrder().then((res) => {\r\n        console.log(res.data.data);\r\n        this.TableList = res.data.data;\r\n        this.TableList.forEach((item) => {\r\n          item.createTime = dayjs(item.createTime).format(\r\n            \"YYYY-MM-DD HH:mm:ss\"\r\n          );\r\n          item.outTime = dayjs(item.outTime).format(\"YYYY-MM-DD HH:mm:ss\");\r\n          item.startTime = dayjs(item.startTime).format(\"YYYY-MM-DD HH:mm:ss\");\r\n          item.endTime = dayjs(item.endTime).format(\"YYYY-MM-DD HH:mm:ss\");\r\n          this.areaList.map((area) => {\r\n            if (item.areaId === area.areaid) {\r\n              item.areaName = area.areaName;\r\n            }\r\n          });\r\n        });\r\n      });\r\n    },\r\n  },\r\n  created() {\r\n    getAllArea().then((res) => {\r\n      console.log(res.data.data);\r\n      this.areaList = res.data.data;\r\n    });\r\n    this.init();\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.goodsManage {\r\n  margin: 10px;\r\n}\r\n\r\n.description {\r\n  // 只显示两行\r\n  display: -webkit-box;\r\n  -webkit-box-orient: vertical;\r\n  -webkit-line-clamp: 2;\r\n  overflow: hidden;\r\n}\r\n</style>"],"mappings":"AAgFA,SACEA,WAAW,EACXC,UAAU,EACVC,cAAc,EACdC,WAAU,QACL,eAAe;AACtB,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE,CAAC;MACdC,MAAM,EAAE,KAAK;MACbC,SAAS,EAAE,CAAC,CAAC;MACbC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACR,SAAS,CAACS,MAAM,CAAC,CAACC,IAAI,EAAEC,KAAK,KAAK;QAC5C,OACEA,KAAI,GAAI,IAAI,CAACV,WAAU,GAAI,EAAC,IAAKU,KAAI,IAAK,CAAC,IAAI,CAACV,WAAU,GAAI,CAAC,IAAI,EAAC;MAExE,CAAC,CAAC;IACJ;EACF,CAAC;EACDW,OAAO,EAAE;IACPC,YAAYA,CAACC,GAAG,EAAE;MAChB,IAAI,CAACC,OAAO,CAAC,2BAA2B,EAAE,IAAI,EAAE,CAChD,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC;QAAEC;MAAM,CAAC,KAAK;QACrB,MAAMlB,IAAG,GAAI;UACbmB,OAAO,EAAEJ,GAAG,CAACI,OAAO;UACpBC,QAAQ,EAAEF;QACZ;QACEpB,WAAW,CAACE,IAAI,CAAC,CAACiB,IAAI,CAAC,MAAM;UAC3B,IAAI,CAACI,IAAI,CAAC;UACV,IAAI,CAACC,QAAQ,CAAC;YACZC,IAAI,EAAE,SAAS;YACfC,OAAO,EAAE,OAAM,GAAIN;UACrB,CAAC,CAAC;QACJ,CAAC;MACH,CAAC,CAAC,CAACO,KAAK,CAAC,MAAM;QACb,IAAI,CAACH,QAAQ,CAAC;UACZC,IAAI,EAAE,MAAM;UACZC,OAAO,EAAE;QACX,CAAC,CAAC;MACJ,CAAC;IACH,CAAC;IACDE,qBAAqBA,CAACC,GAAG,EAAE;MACzB,IAAI,CAACC,UAAS,GAAID,GAAG;IACvB,CAAC;IACDE,UAAUA,CAACd,GAAG,EAAE;MACdlB,cAAc,CAACkB,GAAG,CAACI,OAAO,CAAC,CAACF,IAAI,CAAEa,GAAG,IAAK;QACxCC,OAAO,CAACC,GAAG,CAAC,CAAC;QACb,IAAI,CAAC5B,SAAQ,GAAI0B,GAAG,CAAC9B,IAAI,CAACA,IAAI;QAC9B,IAAI,CAACI,SAAS,CAAC6B,UAAS,GAAIlC,KAAK,CAAC,IAAI,CAACK,SAAS,CAAC6B,UAAU,CAAC,CAACC,MAAM,CAAC,qBAAqB;QACzF,IAAI,CAAC9B,SAAS,CAAC+B,SAAQ,GAAIpC,KAAK,CAAC,IAAI,CAACK,SAAS,CAAC+B,SAAS,CAAC,CAACD,MAAM,CAAC,qBAAqB;QACvF,IAAI,CAAC9B,SAAS,CAACgC,OAAM,GAAIrC,KAAK,CAAC,IAAI,CAACK,SAAS,CAACgC,OAAO,CAAC,CAACF,MAAM,CAAC,qBAAqB;QACnF,IAAI,CAAC9B,SAAS,CAACiC,OAAM,GAAI