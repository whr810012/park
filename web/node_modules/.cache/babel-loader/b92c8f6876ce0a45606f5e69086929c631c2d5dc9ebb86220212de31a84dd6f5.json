{"ast":null,"code":"import { getAllOrder } from '../../api/request';\nimport * as echarts from 'echarts';\nexport default {\n  data() {\n    return {\n      orderList: [],\n      notOrderList: [],\n      awaitOrderList: [],\n      okOrderList: []\n    };\n  },\n  methods: {\n    init() {\n      getAllOrder().then(res => {\n        this.orderList = res.data.data;\n        console.log(this.orderList);\n        this.okOrderList = this.orderList.filter(item => item.status === '1' || item.status === '3');\n        this.notOrderList = this.orderList.filter(item => item.status === '2');\n        this.awaitOrderList = this.orderList.filter(item => item.status === '0');\n      });\n    }\n  },\n  computed: {\n    proportionOption() {\n      return {\n        series: [{\n          type: 'pie',\n          data: [{\n            value: this.okOrderList.length,\n            name: '已确认、已完成订单'\n          }, {\n            value: this.notOrderList.length,\n            name: '已拒绝订单'\n          }, {\n            value: this.awaitOrderList.length,\n            name: '待确认订单'\n          }]\n        }]\n      };\n    }\n  },\n  created() {\n    this.init();\n  },\n  mounted() {\n    const myChart = echarts.init(document.getElementById('proportionOption'));\n    myChart.setOption(this.proportionOption);\n  }\n};","map":{"version":3,"names":["getAllOrder","echarts","data","orderList","notOrderList","awaitOrderList","okOrderList","methods","init","then","res","console","log","filter","item","status","computed","proportionOption","series","type","value","length","name","created","mounted","myChart","document","getElementById","setOption"],"sources":["C:\\Users\\Administrator\\Desktop\\ershou\\src\\pages\\home\\statisticsManage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    各类型订单比例示意图\r\n    <div class=\"proportionOption\" id=\"proportionOption\" style=\"width: 600px;height:400px\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {getAllOrder} from '../../api/request'\r\nimport * as echarts from 'echarts';\r\nexport default {\r\n  data () {\r\n    return {\r\n      orderList: [],\r\n      notOrderList: [],\r\n      awaitOrderList: [],\r\n      okOrderList:[]\r\n    }\r\n  },\r\n  methods:{\r\n    init () {\r\n      getAllOrder().then(res => {\r\n        this.orderList = res.data.data\r\n        console.log(this.orderList)\r\n        this.okOrderList = this.orderList.filter(item => item.status === '1' || item.status === '3')\r\n        this.notOrderList = this.orderList.filter(item => item.status === '2')\r\n        this.awaitOrderList = this.orderList.filter(item => item.status === '0')\r\n      })\r\n    }\r\n  },\r\n  computed:{\r\n    proportionOption () {\r\n      return {\r\n        series:[\r\n          {\r\n            type: 'pie',\r\n            data:[\r\n              {\r\n                value: this.okOrderList.length,\r\n                name:'已确认、已完成订单'\r\n              },\r\n              {\r\n                value: this.notOrderList.length,\r\n                name:'已拒绝订单'\r\n              },\r\n              {\r\n                value: this.awaitOrderList.length,\r\n                name:'待确认订单'\r\n              }\r\n            ]\r\n          }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    this.init()\r\n  },\r\n  mounted () {\r\n    const myChart = echarts.init(document.getElementById('proportionOption'))\r\n    myChart.setOption(this.proportionOption)\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAQA,SAAQA,WAAW,QAAO,mBAAkB;AAC5C,OAAO,KAAKC,OAAM,MAAO,SAAS;AAClC,eAAe;EACbC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE,EAAE;MAChBC,cAAc,EAAE,EAAE;MAClBC,WAAW,EAAC;IACd;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,IAAGA,CAAA,EAAK;MACNR,WAAW,CAAC,CAAC,CAACS,IAAI,CAACC,GAAE,IAAK;QACxB,IAAI,CAACP,SAAQ,GAAIO,GAAG,CAACR,IAAI,CAACA,IAAG;QAC7BS,OAAO,CAACC,GAAG,CAAC,IAAI,CAACT,SAAS;QAC1B,IAAI,CAACG,WAAU,GAAI,IAAI,CAACH,SAAS,CAACU,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,MAAK,KAAM,GAAE,IAAKD,IAAI