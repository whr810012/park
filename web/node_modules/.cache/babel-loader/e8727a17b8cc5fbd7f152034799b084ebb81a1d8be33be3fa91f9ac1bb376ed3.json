{"ast":null,"code":"import dayjs from 'dayjs';\nimport { getAllArea, getAllOrder, getAllUser, getComment } from '@/api/request';\nexport default {\n  name: 'evaluateManage',\n  data() {\n    return {\n      commentList: [],\n      userList: [],\n      orderList: [],\n      areaList: [],\n      radio: '全部'\n    };\n  },\n  computed: {\n    commentListFilter() {\n      console.log(this.radio);\n      return this.commentList.filter(item => {\n        if (this.radio === '全部') {\n          return true;\n        } else {\n          return item.areaid == this.radio;\n        }\n      });\n    }\n  },\n  methods: {\n    async init() {\n      getComment().then(res => {\n        this.commentList = res.data.data;\n        this.commentList.forEach(item => {\n          item.createTime = dayjs(item.createTime).format('YYYY-MM-DD HH:mm:ss');\n        });\n        getAllUser().then(res => {\n          this.userList = res.data.data;\n          this.userList.map(item => {\n            this.commentList.forEach(item1 => {\n              if (item.openId == item1.openId) {\n                item1.userName = item.userName;\n              }\n            });\n          });\n        });\n        console.log(this.commentList);\n      });\n      getAllOrder().then(res => {\n        this.orderList = res.data.data;\n      });\n      getAllArea().then(res => {\n        this.areaList = res.data.data;\n      });\n    }\n  },\n  created() {\n    this.init();\n  }\n};","map":{"version":3,"names":["dayjs","getAllArea","getAllOrder","getAllUser","getComment","name","data","commentList","userList","orderList","areaList","radio","computed","commentListFilter","console","log","filter","item","areaid","methods","init","then","res","forEach","createTime","format","map","item1","openId","userName","created"],"sources":["D:\\CODE\\park\\park-vue\\web\\src\\pages\\home\\evaluateManage.vue"],"sourcesContent":["<template>\r\n  <div class=\"evaluateManage\">\r\n    <el-radio-group v-model=\"radio\">\r\n      <el-radio-button label=\"全部\"></el-radio-button>\r\n      <el-radio-button v-for=\"item in areaList\" :key=\"item.areaid\" :value=\"item.areaid\" :label=\"item.areaName\"></el-radio-button>\r\n    </el-radio-group>\r\n    <div class=\"content\" style=\"width:100%\">\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"6\" v-for=\"item in commentListFilter\" :key=\"item.evaluateId\" style=\"min-width: 300px !important;margin-top:10px\">\r\n          <el-card class=\"box-card\" shadow=\"hover\">\r\n            <div>\r\n              用户：{{ item.userName }}\r\n            </div>\r\n            <el-popover placement=\"top-start\" trigger=\"hover\" :content=\"item.body\">\r\n              <template #reference>\r\n                <div class=\"body-centet\"> 内容：\r\n                    {{ item.body }}\r\n                </div>\r\n              </template>\r\n            </el-popover>\r\n            <div>评分：{{ item.score }}</div>\r\n            <div>时间：{{ item.createTime }}</div>\r\n          </el-card>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport dayjs from 'dayjs'\r\nimport { getAllArea, getAllOrder, getAllUser, getComment } from '@/api/request'\r\nexport default {\r\n  name: 'evaluateManage',\r\n  data() {\r\n    return {\r\n      commentList: [],\r\n      userList: [],\r\n      orderList: [],\r\n      areaList: [],\r\n      radio: '全部'\r\n    }\r\n  },\r\n  computed: {\r\n    commentListFilter() {\r\n      console.log(this.radio)\r\n      return this.commentList.filter(item => {\r\n        if (this.radio === '全部') {\r\n          return true\r\n        } else {\r\n          return item.areaid == this.radio\r\n        }\r\n      })\r\n    }\r\n  },\r\n  methods: {\r\n    async init() {\r\n      getComment().then(res => {\r\n        this.commentList = res.data.data\r\n        this.commentList.forEach(item => {\r\n          item.createTime = dayjs(item.createTime).format('YYYY-MM-DD HH:mm:ss')\r\n        })\r\n        getAllUser().then(res => {\r\n          this.userList = res.data.data\r\n          this.userList.map(item => {\r\n            this.commentList.forEach(item1 => {\r\n              if (item.openId == item1.openId) {\r\n                item1.userName = item.userName\r\n              }\r\n            })\r\n          })\r\n        })\r\n        console.log(this.commentList);\r\n      })\r\n      getAllOrder().then(res => {\r\n        this.orderList = res.data.data\r\n      })\r\n      getAllArea().then(res => {\r\n        this.areaList = res.data.data\r\n      })\r\n    }\r\n  },\r\n  created() {\r\n    this.init()\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.evaluateManage {\r\n  padding: 20px\r\n}\r\n\r\n.content {\r\n  margin-top: 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  flex-wrap: wrap;\r\n\r\n  .body-centet {\r\n    display: -webkit-box;\r\n    -webkit-box-orient: vertical;\r\n    -webkit-line-clamp: 3;\r\n    overflow: hidden;\r\n  }\r\n}\r\n</style>"],"mappings":"AA8BA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,UAAU,EAAEC,WAAW,EAAEC,UAAU,EAAEC,UAAS,QAAS,eAAc;AAC9E,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,iBAAiBA,CAAA,EAAG;MAClBC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACJ,KAAK;MACtB,OAAO,IAAI,CAACJ,WAAW,CAACS,MAAM,CAACC,IAAG,IAAK;QACrC,IAAI,IAAI,CAACN,KAAI,KAAM,IAAI,EAAE;UACvB,OAAO,IAAG;QACZ,OAAO;UACL,OAAOM,IAAI,CAACC,MAAK,IAAK,IAAI,CAACP,KAAI;QACjC;MACF,CAAC;IACH;EACF,CAAC;EACDQ,OAAO,EAAE;IACP,MAAMC,IAAIA,CAAA,EAAG;MACXhB,UAAU,CAAC,CAAC,CAACiB,IAAI,CAACC,GAAE,IAAK;QACvB,IAAI,CAACf,WAAU,GAAIe,GAAG,CAAChB,IAAI,CAACA,IAAG;QAC/B,IAAI,CAACC,WAAW,CAACgB,OAAO,CAACN,IAAG,IAAK;UAC/BA,IAAI,CAACO,UAAS,GAAIxB,KAAK,CAACiB,IAAI,CAACO,UAAU,CAAC,CAACC,MAAM,CAAC,qBAAqB;QACvE,CAAC;QACDtB,UAAU,CAAC,CAAC,CAACkB,IAAI,CAACC,GAAE,IAAK;UACvB,IAAI,CAACd,QAAO,GAAIc,GAAG,CAAChB,IAAI,CAACA,IAAG;UAC5B,I