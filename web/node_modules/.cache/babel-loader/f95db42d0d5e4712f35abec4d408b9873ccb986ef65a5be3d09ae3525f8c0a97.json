{"ast":null,"code":"// import homeNav from './home-nav.vue'\nimport HomeMenu from \"../components/menu.vue\";\nimport breadCrumb from \"../components/breadCrumb.vue\";\nimport { getData } from \"../api/request\";\nexport default {\n  components: {\n    // homeNav\n    breadCrumb,\n    HomeMenu\n  },\n  data() {\n    return {\n      menuHeight: {},\n      mainHeight: {},\n      headerImg: \"/menu/header.png\"\n    };\n  },\n  mounted() {\n    getData();\n    this.$electron.ipcRenderer.on(\"window-size\", (event, size) => {\n      console.log(size.height); // { width, height }\n      this.mainHeight = {\n        height: `${size.height - 150}px`\n      };\n    });\n    // 在这里更新Vue组件的窗口大小状态\n    this.$electron.ipcRenderer.on(\"window-resize\", (event, size) => {\n      // console.log(size); // { width, height }\n      this.mainHeight = {\n        height: `${size.height - 150}px`\n      };\n    });\n    this.$electron.ipcRenderer.send(\"get-window-size\");\n  },\n  methods: {\n    handleOpen(key, keyPath) {\n      console.log(key, keyPath);\n    },\n    handleClose(key, keyPath) {\n      console.log(key, keyPath);\n    },\n    changeMenu() {\n      this.isCollapse = !this.isCollapse;\n    },\n    start() {\n      console.log(\"123123\");\n      this.$electron.ipcRenderer.on(\"start-browser\", e => {\n        console.log(\"=============\", e);\n        this.$message(\"正在启动浏览器\");\n      });\n      this.$electron.ipcRenderer.send(\"start\");\n    }\n  }\n};","map":{"version":3,"names":["HomeMenu","breadCrumb","getData","components","data","menuHeight","mainHeight","headerImg","mounted","$electron","ipcRenderer","on","event","size","console","log","height","send","methods","handleOpen","key","keyPath","handleClose","changeMenu","isCollapse","start","e","$message"],"sources":["C:\\Users\\Administrator\\Documents\\WeChat Files\\wxid_9s69en6iup1p22\\FileStorage\\File\\2024-10\\web\\web\\src\\pages\\home.vue"],"sourcesContent":["<template>\n  <div class=\"main\">\n    <el-container>\n      <el-aside width=\"150px\">\n        <home-menu></home-menu>\n      </el-aside>\n      <el-container>\n        <el-header>\n          <el-image class=\"header\" :src=\"headerImg\"></el-image>\n          <bread-crumb></bread-crumb>\n        </el-header>\n        <el-main :style=\"mainHeight\">\n          <!-- <div @click=\"start\">-------========</div> -->\n          <router-view></router-view>\n        </el-main>\n      </el-container>\n    </el-container>\n  </div>\n</template>\n\n<script>\n// import homeNav from './home-nav.vue'\nimport HomeMenu from \"../components/menu.vue\";\nimport breadCrumb from \"../components/breadCrumb.vue\";\nimport { getData } from \"../api/request\";\nexport default {\n  components: {\n    // homeNav\n    breadCrumb,\n    HomeMenu,\n  },\n  data() {\n    return {\n      menuHeight: {},\n      mainHeight: {},\n      headerImg: \"/menu/header.png\",\n    };\n  },\n  mounted() {\n    getData();\n    this.$electron.ipcRenderer.on(\"window-size\", (event, size) => {\n      console.log(size.height); // { width, height }\n      this.mainHeight = {\n        height: `${size.height - 150}px`,\n      };\n    });\n    // 在这里更新Vue组件的窗口大小状态\n    this.$electron.ipcRenderer.on(\"window-resize\", (event, size) => {\n      // console.log(size); // { width, height }\n      this.mainHeight = {\n        height: `${size.height - 150}px`,\n      };\n    });\n\n    this.$electron.ipcRenderer.send(\"get-window-size\");\n  },\n  methods: {\n    handleOpen(key, keyPath) {\n      console.log(key, keyPath);\n    },\n    handleClose(key, keyPath) {\n      console.log(key, keyPath);\n    },\n    changeMenu() {\n      this.isCollapse = !this.isCollapse;\n    },\n    start() {\n      console.log(\"123123\");\n      this.$electron.ipcRenderer.on(\"start-browser\", (e) => {\n        console.log(\"=============\", e);\n        this.$message(\"正在启动浏览器\");\n      });\n      this.$electron.ipcRenderer.send(\"start\");\n    },\n  },\n};\n</script>\n\n<style lang=