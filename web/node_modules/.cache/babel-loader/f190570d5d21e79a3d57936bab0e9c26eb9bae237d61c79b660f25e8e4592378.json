         <el-input v-model=\"form.lng\" autocomplete=\"off\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"名称:\" :label-width=\"formLabelWidth\">\r\n                    <el-input v-model=\"form.name\" autocomplete=\"off\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"类型:\" :label-width=\"formLabelWidth\">\r\n                    <el-input v-model=\"form.icon\" autocomplete=\"off\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"详情:\" :label-width=\"formLabelWidth\">\r\n                    <el-input v-model=\"form.detail\" autocomplete=\"off\" type=\"textarea\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"图片:\" :label-width=\"formLabelWidth\">\r\n                    <el-upload action=\"#\" v-model:file-list=\"form.image\" :auto-upload=\"false\" limit=\"3\"\r\n                        list-type=\"picture-card\" :on-preview=\"handlePictureCardPreview\" :on-remove=\"handleRemove\"\r\n                        :on-exceed=\"handleExceed\" :on-change=\"handleChange\" accept=\"image/jpg,image/jpeg,image/png\">\r\n                        <el-icon>\r\n                            <Plus />\r\n                        </el-icon>\r\n                    </el-upload>\r\n                    <el-dialog v-model=\"imageshow\">\r\n                        <img w-full :src=\"dialogImageUrl\" alt=\"Preview Image\" />\r\n                    </el-dialog>\r\n                </el-form-item>\r\n            </el-form>\r\n            <template #footer>\r\n                <div class=\"dialog-footer\">\r\n                    <el-button @click=\"dialogFormVisible = false\">取消</el-button>\r\n                    <el-button type=\"primary\" @click=\"uploadFrom\">\r\n                        确认\r\n                    </el-button>\r\n                </div>\r\n            </template>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { getAllAdmin, deleteAdmin } from \"../../api/request.js\";\r\nexport default {\r\n    name: \"marksManage\",\r\n    data() {\r\n        return {\r\n            imageshow: false,\r\n            dialogFormVisible: false,\r\n            form: {\r\n                id: '',\r\n                lat: '',\r\n                lng: '',\r\n                name: '',\r\n                icon: '',\r\n                detail: '',\r\n                image: ''\r\n            },\r\n            tableData: [],\r\n            multipleSelection: [],\r\n        }\r\n    },\r\n    computed: {\r\n      myInfo () {\r\n        return this.$store.state.MyInfo\r\n      }    \r\n    },\r\n    methods: {\r\n        handleSelectionChange(val) {\r\n            this.multipleSelection = val\r\n        },\r\n        deleteMarkers() {\r\n            console.log(this.multipleSelection);\r\n            if (this.multipleSelection.length == 0) {\r\n                this.$message.error('请选择要删除的标记')\r\n                return\r\n            }\r\n            if (this.myInfo.sorct !== 0) {\r\n                this.$message.error('无权限删除')\r\n            }\r\n            const ids = this.multipleSelection.map(item => item.adminid)\r\n            if ( this.multipleSelection.find(item => item.sorct == 0)) {\r\n                console.log('不可删除超级管理员');\r\n            }\r\n            this.$confirm('确定删除？', '提示', {\r\n                confirmButtonText: '确定',\r\n                cancelButtonText: '取消',\r\n                type: 'warning'\r\n            }).then(() => {\r\n                console.log(ids);\r\n                deleteAdmin({ids: ids}).then(res => {\r\n                    this.init()\r\n                })\r\n            })\r\n        },\r\n        init() {\r\n            getAllAdmin().then(res => {\r\n                console.log(res.data.data)\r\n                this.tableData = res.data.data\r\n            })\r\n        }\r\n\r\n    },\r\n    created() {\r\n        this.$store.commit(\"setBreadList\", [{ title: '管理员管理' }]);\r\n        this.init()\r\n    },\r\n}\r\n</script>"],"mappings":"AA+DA,SAASA,WAAW,EAAEC,WAAU,QAAS,sBAAsB;AAC/D,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE,KAAK;MAChBC,iBAAiB,EAAE,KAAK;MACxBC,IAAI,EAAE;QACFC,EAAE,EAAE,EAAE;QACNC,GAAG,EAAE,EAAE;QACPC,GAAG,EAAE,EAAE;QACPP,IAAI,EAAE,EAAE;QACRQ,IAAI,EAAE,EAAE;QACRC,MAAM,EAAE,EAAE;QACVC,KAAK,EAAE;MACX,CAAC;MACDC,SAAS,EAAE,EAAE;MACbC,iBAAiB,EAAE;IACvB;EACJ,CAAC;EACDC,QAAQ,EAAE;IACRC,MAAKA,CAAA,EAAK;MACR,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,MAAK;IAChC;EACF,CAAC;EACDC,OAAO,EAAE;IACLC,qBAAqBA,CAACC,GAAG,EAAE;MACvB,IAAI,CAACR,iBAAgB,GAAIQ,GAAE;IAC/B,CAAC;IACDC,aAAaA,CAAA,EAAG;MACZC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACX,iBAAiB,CAAC;MACnC,IAAI,IAAI,CAACA,iBAAiB,CAACY,MAAK,IAAK,CAAC,EAAE;QACpC,IAAI,CAACC,QAAQ,CAACC,KAAK,CAAC,WAAW;QAC/B;MACJ;MACA,IAAI,IAAI,CAACZ,MAAM,CAACa,KAAI,KAAM,CAAC,EAAE;QACzB,IAAI,CAACF,QAAQ,CAACC,KAAK,CAAC,OAAO;MAC/B;MACA,MAAME,GAAE,GAAI,IAAI,CAAChB,iBAAiB,CAACiB,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACC,OAAO;MAC3D,IAAK,IAAI,CAACnB,iBAAiB,CAACoB,IAAI,CAACF,IAAG,IAAKA,IAAI,CAACH,KAAI,IAAK,CAAC,CAAC,EAAE;QACvDL,OAAO,CAACC,GAAG,CAAC,WAAW,CAAC;MAC5B;MACA,IAAI,CAACU,QAAQ,CAAC,OAAO,EAAE,IAAI,EAAE;QACzBC,iBAAiB,EAAE,IAAI;QACvBC,gBAAgB,EAAE,IAAI;QACtBC,IAAI,EAAE;MACV,CAAC,CAAC,CAACC,IAAI,CAAC,MAAM;QACVf,OAAO,CAACC,GAAG,CAACK,GAAG,CAAC;QAChB7B,WAAW,CAAC;UAAC6B,GAAG,EAAEA;QAAG,CAAC,CAAC,CAACS,IAAI,CAACC,GAAE,IAAK;UAChC,IAAI,CAACC,IAAI,CAAC;QACd,CAAC;MACL,CAAC;IACL,CAAC;IACDA,IAAIA,CAAA,EAAG;MACHzC,WAAW,CAAC,CAAC,CAACuC,IAAI,CAACC,GAAE,IAAK;QACtBhB,OAAO,CAACC,GAAG,CAACe,GAAG,CAACrC,IAAI,CAACA,IAAI;QACzB,IAAI,CAACU,SAAQ,GAAI2B,GAAG,CAACrC,IAAI,CAACA,IAAG;MACjC,CAAC;IACL;EAEJ,CAAC;EACDuC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACzB,MAAM,CAAC0B,MAAM,CAAC,cAAc,EAAE,CAAC;MAAEC,KAAK,EAAE;IAAQ,CAAC,CAAC,CAAC;IACxD,IAAI,CAACH,IAAI,CAAC;EACd;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}