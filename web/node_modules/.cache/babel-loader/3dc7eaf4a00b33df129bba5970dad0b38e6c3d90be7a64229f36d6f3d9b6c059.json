{"ast":null,"code":"import { getAllOrder, getAllArea, getAllPark, getAllUser, getOrderDetail } from \"@/api/request\";\nimport dayjs from \"dayjs\";\nexport default {\n  data() {\n    return {\n      TableList: [],\n      currentPage: 1,\n      drawer: false,\n      orderInfo: {},\n      areaList: [],\n      stallList: [],\n      userList: []\n    };\n  },\n  computed: {\n    filterList() {\n      return this.TableList.filter((item, index) => {\n        return index < this.currentPage * 10 && index >= (this.currentPage - 1) * 10;\n      });\n    }\n  },\n  methods: {\n    handleSelectionChange(val) {\n      this.checkGoods = val;\n    },\n    lookDetail(row) {\n      getOrderDetail(row.orderid).then(res => {\n        console.log();\n        this.orderInfo = res.data.data;\n        this.orderInfo.createTime = dayjs(this.orderInfo.createTime).format(\"YYYY-MM-DD HH:mm:ss\");\n        this.orderInfo.startTime = dayjs(this.orderInfo.startTime).format(\"YYYY-MM-DD HH:mm:ss\");\n        this.orderInfo.outTime = dayjs(this.orderInfo.outTime).format(\"YYYY-MM-DD HH:mm:ss\");\n        this.orderInfo.endTime = dayjs(this.orderInfo.endTime).format(\"YYYY-MM-DD HH:mm:ss\");\n        this.areaList.map(area => {\n          if (this.orderInfo.areaid === area.areaid) {\n            this.orderInfo.areaName = area.areaName;\n          }\n        });\n        this.drawer = true;\n      });\n      // this.orderInfo = row\n      // console.log(this.orderInfo);\n      // this.areaList.map(area => {\n      //   if (this.orderInfo.areaId === area.areaid) {\n      //     this.orderInfo.areaName = area.areaName\n      //   }\n      // })\n      // this.stallList.map(stall => {\n      //   if (this.orderInfo.stallId === stall.stallId) {\n      //     this.orderInfo.location = stall.location\n      //   }\n      // })\n      // this.userList.map(user => {\n      //   if (this.orderInfo.openId === user.openid) {\n      //     this.orderInfo.userName = user.userName\n      //     this.orderInfo.carInfo = user.carInfo\n      //   }\n      // })\n      // console.log(this.orderInfo);\n      // this.drawer = true\n    },\n    init() {\n      getAllOrder().then(res => {\n        console.log(res.data.data);\n        this.TableList = res.data.data;\n        this.TableList.forEach(item => {\n          item.createTime = dayjs(item.createTime).format(\"YYYY-MM-DD HH:mm:ss\");\n          item.outTime = dayjs(item.outTime).format(\"YYYY-MM-DD HH:mm:ss\");\n          item.startTime = dayjs(item.startTime).format(\"YYYY-MM-DD HH:mm:ss\");\n          item.endTime = dayjs(item.endTime).format(\"YYYY-MM-DD HH:mm:ss\");\n          this.areaList.map(area => {\n            if (item.areaId === area.areaid) {\n              item.areaName = area.areaName;\n            }\n          });\n        });\n      });\n    }\n  },\n  created() {\n    getAllArea().then(res => {\n      console.log(res.data.data);\n      this.areaList = res.data.data;\n    });\n    getAllPark().then(res => {\n      this.stallList = res.data.data;\n    });\n    getAllUser().then(res => {\n      this.userList = res.data.data;\n    });\n    this.init();\n  }\n};","map":{"version":3,"names":["getAllOrder","getAllArea","getAllPark","getAllUser","getOrderDetail","dayjs","data","TableList","currentPage","drawer","orderInfo","areaList","stallList","userList","computed","filterList","filter","item","index","methods","handleSelectionChange","val","checkGoods","lookDetail","row","orderid","then","res","console","log","createTime","format","startTime","outTime","endTime","map","area","areaid","areaName","init","forEach","areaId","created"],"sources":["D:\\CODE\\park\\park-vue\\web\\src\\pages\\home\\ordersManage.vue"],"sourcesContent":["<template>\r\n  <div class=\"goodsManage\">\r\n    <el-table :data=\"filterList\" style=\"width: 100%\" border>\r\n      <el-table-column\r\n        label=\"序号\"\r\n        width=\"120\"\r\n        align=\"center\"\r\n        type=\"index\"\r\n      ></el-table-column>\r\n      <el-table-column property=\"orderid\" label=\"订单号\" align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column property=\"createTime\" label=\"创建时间\"></el-table-column>\r\n      <el-table-column prop=\"outTime\" label=\"离场时间\"></el-table-column>\r\n      <el-table-column prop=\"startTime\" label=\"开始时间\"></el-table-column>\r\n      <el-table-column prop=\"endTime\" label=\"结束时间\"></el-table-column>\r\n      <el-table-column prop=\"totalPrice\" label=\"总价格\"></el-table-column>\r\n      <el-table-column prop=\"status\" label=\"状态\">\r\n        <template #default=\"scope\">\r\n          <el-tag type=\"primary\" v-if=\"scope.row.status === 0\">预定中</el-tag>\r\n          <el-tag type=\"success\" v-if=\"scope.row.status === 1\">进行中</el-tag>\r\n          <el-tag type=\"danger\" v-if=\"scope.row.status === 2\">已超时</el-tag>\r\n          <el-tag type=\"info\" v-if=\"scope.row.status === 3\">已完成</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\r\n        <template #default=\"scope\">\r\n          <el-button type=\"text\" size=\"small\" @click=\"lookDetail(scope.row)\"\r\n            >查看详情</el-button\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"margin-top: 20px\">\r\n      <el-pagination\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :total=\"TableList.length\"\r\n        v-model:current-page=\"currentPage\"\r\n      ></el-pagination>\r\n    </div>\r\n    <el-drawer\r\n      v-model=\"drawer\"\r\n      title=\"I am the title\"\r\n      :with-header=\"false\"\r\n      width=\"300px\"\r\n    >\r\n      <h3>订单详情</h3>\r\n      <div style=\"margin-top: 10px\">订单号： {{ orderInfo.orderid }}</div>\r\n      <div style=\"margin-top: 10px\">买家： {{ orderInfo.userName }}</div>\r\n      <div style=\"margin-top: 10px\">车辆信息： {{ orderInfo.carInfo }}</div>\r\n      <div style=\"margin-top: 10px\">\r\n        停放位置： {{ orderInfo.areaName }} - {{ orderInfo.location }}\r\n      </div>\r\n      <div style=\"margin-top: 10px\">\r\n        订单创建时间： {{ orderInfo.createTime }}\r\n      </div>\r\n      <div style=\"margin-top: 10px\">\r\n        订单开始时间： {{ orderInfo.startTime }}\r\n      </div>\r\n      <div style=\"margin-top: 10px\">订单结束时间： {{ orderInfo.endTime }}</div>\r\n      <div style=\"margin-top: 10px\">车主离开时间： {{ orderInfo.outTime }}</div>\r\n      <div style=\"margin-top: 10px\">\r\n        订单状态：\r\n        <span>\r\n          <el-tag type=\"primary\" v-if=\"orderInfo.status === 0\">预定中</el-tag>\r\n          <el-tag type=\"success\" v-if=\"orderInfo.status === 1\">进行中</el-tag>\r\n          <el-tag type=\"danger\" v-if=\"orderInfo.status === 2\">已超时</el-tag>\r\n          <el-tag type=\"info\" v-if=\"orderInfo.status === 3\">已完成</el-tag>\r\n        </span>\r\n      </div>\r\n    </el-drawer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  getAllOrder,\r\n  getAllArea,\r\n  getAllPark,\r\n  getAllUser,\r\n  getOrderDetail,\r\n} from \"@/api/request\";\r\nimport dayjs from \"dayjs\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      TableList: [],\r\n      currentPage: 1,\r\n      drawer: false,\r\n      orderInfo: {},\r\n      areaList: [],\r\n      stallList: [],\r\n      userList: [],\r\n    };\r\n  },\r\n  computed: {\r\n    filterList() {\r\n      return this.TableList.filter((item, index) => {\r\n        return (\r\n          index < this.currentPage * 10 && index >= (this.currentPage - 1) * 10\r\n        );\r\n      });\r\n    },\r\n  },\r\n  methods: {\r\n    handleSelectionChange(val) {\r\n      this.checkGoods = val;\r\n    },\r\n    lookDetail(row) {\r\n      getOrderDetail(row.orderid).then((res) => {\r\n        console.log();\r\n        this.orderInfo = res.data.data;\r\n        this.orderInfo.createTime = dayjs(this.orderInfo.createTime).format(\"YYYY-MM-DD HH:mm:ss\")\r\n        this.orderInfo.startTime = dayjs(this.orderInfo.startTime).format(\"YYYY-MM-DD HH:mm:ss\")\r\n        this.orderInfo.outTime = dayjs(this.orderInfo.outTime).format(\"YYYY-MM-DD HH:mm:ss\")\r\n        this.orderInfo.endTime = dayjs(this.orderInfo.endTime).format(\"YYYY-MM-DD HH:mm:ss\")\r\n        this.areaList.map((area) => {\r\n          if (this.orderInfo.areaid === area.areaid) {\r\n            this.orderInfo.areaName = area.areaName;\r\n          }\r\n        });\r\n        this.drawer = true;\r\n      });\r\n      // this.orderInfo = row\r\n      // console.log(this.orderInfo);\r\n      // this.areaList.map(area => {\r\n      //   if (this.orderInfo.areaId === area.areaid) {\r\n      //     this.orderInfo.areaName = area.areaName\r\n      //   }\r\n      // })\r\n      // this.stallList.map(stall => {\r\n      //   if (this.orderInfo.stallId === stall.stallId) {\r\n      //     this.orderInfo.location = stall.location\r\n      //   }\r\n      // })\r\n      // this.userList.map(user => {\r\n      //   if (this.orderInfo.openId === user.openid) {\r\n      //     this.orderInfo.userName = user.userName\r\n      //     this.orderInfo.carInfo = user.carInfo\r\n      //   }\r\n      // })\r\n      // console.log(this.orderInfo);\r\n      // this.drawer = true\r\n    },\r\n    init() {\r\n      getAllOrder().then((res) => {\r\n        console.log(res.data.data);\r\n        this.TableList = res.data.data;\r\n        this.TableList.forEach((item) => {\r\n          item.createTime = dayjs(item.createTime).format(\r\n            \"YYYY-MM-DD HH:mm:ss\"\r\n          );\r\n          item.outTime = dayjs(item.outTime).format(\"YYYY-MM-DD HH:mm:ss\");\r\n          item.startTime = dayjs(item.startTime).format(\"YYYY-MM-DD HH:mm:ss\");\r\n          item.endTime = dayjs(item.endTime).format(\"YYYY-MM-DD HH:mm:ss\");\r\n          this.areaList.map((area) => {\r\n            if (item.areaId === area.areaid) {\r\n              item.areaName = area.areaName;\r\n            }\r\n          });\r\n        });\r\n      });\r\n    },\r\n  },\r\n  created() {\r\n    getAllArea().then((res) => {\r\n      console.log(res.data.data);\r\n      this.areaList = res.data.data;\r\n    });\r\n    getAllPark().then((res) => {\r\n      this.stallList = res.data.data;\r\n    });\r\n    getAllUser().then((res) => {\r\n      this.userList = res.data.data;\r\n    });\r\n    this.init();\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.goodsManage {\r\n  margin: 10px;\r\n}\r\n\r\n.description {\r\n  // 只显示两行\r\n  display: -webkit-box;\r\n  -webkit-box-orient: vertical;\r\n  -webkit-line-clamp: 2;\r\n  overflow: hidden;\r\n}\r\n</style>"],"mappings":"AA2EA,SACEA,WAAW,EACXC,UAAU,EACVC,UAAU,EACVC,UAAU,EACVC,cAAc,QACT,eAAe;AACtB,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE,CAAC;MACdC,MAAM,EAAE,KAAK;MACbC,SAAS,EAAE,CAAC,CAAC;MACbC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACR,SAAS,CAACS,MAAM,CAAC,CAACC,IAAI,EAAEC,KAAK,KAAK;QAC5C,OACEA,KAAI,GAAI,IAAI,CAACV,WAAU,GAAI,EAAC,IAAKU,KAAI,IAAK,CAAC,IAAI,CAACV,WAAU,GAAI,CAAC,IAAI,EAAC;MAExE,CAAC,CAAC;IACJ;EACF,CAAC;EACDW,OAAO,EAAE;IACPC,qBAAqBA,CAACC,GAAG,EAAE;MACzB,IAAI,CAACC,UAAS,GAAID,GAAG;IACvB,CAAC;IACDE,UAAUA,CAACC,GAAG,EAAE;MACdpB,cAAc,CAACoB,GAAG,CAACC,OAAO,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;QACxCC,OAAO,CAACC,GAAG,CAAC,CAAC;QACb,IAAI,CAACnB,SAAQ,GAAIiB,GAAG,CAACrB,IAAI,CAACA,IAAI;QAC9B,IAAI,CAACI,SAAS,CAACoB,UAAS,GAAIzB,KAAK,CAAC,IAAI,CAACK,SAAS,CAACoB,UAAU,CAAC,CAACC,MAAM,CAAC,qBAAqB;QACzF,IAAI,CAACrB,SAAS,CAACsB,SAAQ,GAAI3B,KAAK,CAAC,IAAI,CAACK,SAAS,CAACsB,SAAS,CAAC,CAACD,MAAM,CAAC,qBAAqB;QACvF,IAAI,CAACrB,SAAS,CAACuB,OAAM,GAAI5B,KAAK,CAAC,IAAI,CAACK,SAAS,CAACuB,OAAO,CAAC,CAACF,MAAM,CAAC,qBAAqB;QACnF,IAAI,CAACrB,SAAS,CAACwB,OAAM,GAAI7B,KAAK,CAAC,IAAI,CAACK,SAAS,CAACwB,OAAO,CAAC,CAACH,MAAM,CAAC,qBAAqB;QACnF,IAAI,CAACpB,QAAQ,CAACwB,GAAG,CAAEC,IAAI,IAAK;UAC1B,IAAI,IAAI,CAAC1B,SAAS,CAAC2B,MAAK,KAAMD,IAAI,CAACC,MAAM,EAAE;YACzC,IAAI,CAAC3B,SAAS,C